{
  "routes": [
    {
      "route": "/*",
      "allowedRoles": ["authenticated"]
    }
  ],
  "responseOverrides": {
    "401": {
      "statusCode": 302,
      "redirect": "/.auth/login/aadb2c"
    }
  },
  "auth": {
    "identityProviders": {
      "aadb2c": {
        "registration": {
          "openIdIssuer": "https://login.microsoftonline.com/86d2b60e-0a55-4cd4-b7dc-8961288e385e/v2.0",
          "clientIdSettingName": "AZURE_CLIENT_ID",
          "clientSecretSettingName": "AZURE_CLIENT_SECRET_APP_SETTING_NAME"
        }
      }
    }
  }
}